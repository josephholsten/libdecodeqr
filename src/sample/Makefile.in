CXX=@CXX@
CPPFLAGS=@CPPFLAGS@ @CXXFLAGS@ -c

LIBCV=@LIBCV@
LIBHIGHGUI=@LIBHIGHGUI@

LDFLAGS=@LDFLAGS@

export CXX CPPFLAGS LDFLAGS LIBCV LIBHIGHGUI

dirs:=simple webcam
.PHONY: $(dirs)

all: $(dirs)

$(dirs):
	cd $@ && $(MAKE)

install: $(dirs)

clean:
	for x in $(dirs); do cd $$x && $(MAKE) clean && cd ../;done


